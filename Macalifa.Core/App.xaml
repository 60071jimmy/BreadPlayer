                                        </MenuFlyoutSubItem>-->
                                        <MenuFlyoutItem Text="Open Song Location" DataContext="{StaticResource LibVM}" Command="{Binding OpenSongLocationCommand}" CommandParameter="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=Content}"/>
                                        <MenuFlyoutItem Text="Delete" DataContext="{StaticResource LibVM}" Tag="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}}" Command="{Binding DeleteCommand}" CommandParameter="{Binding RelativeSource={RelativeSource Mode=Self}}">

                                        </MenuFlyoutItem>
                                        <MenuFlyoutItem Text="Properties"/>

